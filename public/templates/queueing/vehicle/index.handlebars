{{#when vehicle}}
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Registration</th>
            <th>Weight Class</th>
        </tr>
    </thead>
    <tbody>
        {{#each vehicle}}
        <tr>
            <td>{{id}}</td>
            <td>{{regNo}}</td>
            <td>{{weightCategory}}</td>
        </tr>
        {{/each}}
     </tbody>
</table>
{{else}}
No vehicles.
{{/when}}
