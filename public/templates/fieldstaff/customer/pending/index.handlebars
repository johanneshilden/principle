{{#when customer}}
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Phone</th>
            <th>Address</th>
            <th>Created</th>
        </tr>
    </thead>
    <tbody>
        {{#each customer}}
        <tr>
            <td>{{id}}</td>
            <td>{{name}}</td>
            <td>{{phone}}</td>
            <td>{{address}}</td>
            <td>{{created}}</td>
            <td><a href="#!f/customer/register/{{id}}">Register</a></td>
        </tr>
        {{/each}}
    </tbody>
</table>
{{else}}
No pending customer registrations.
{{/when}}
